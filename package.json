{
  "name": "cheapreats",
  "version": "0.0.1",
  "description": "CheaprEats customer mobile app.",
  "author": "CheaprEats",
  "private": true,
  "scripts": {
    "/* setup:macbook */": "Sets up Macbook Computers for IOS App Development",
    "setup:macbook": "npm install && cd ios/ && sudo gem install cocoapods && pod install && cd ..",
    "/* setup:macbook:test */": "Sets up Macbook Computers for IOS App Test Development - *Testing only works on Macs*",
    "setup:macbook:test": "npm install && cd ios/ && sudo gem install cocoapods && pod install && cd .. && npm run build:ios && brew tap wix/brew && brew install applesimutils && npm run detox:build",
    "/* setup:windows */": "Sets up Windows Computers for Android App Development",
    "setup:windows": "npm install && cd android/ && echo sdk.dir=/Users/YOUR_USERNAME/Library/Android/sdk > local.properties && cd .. && export JAVA_HOME=$(/usr/libexec/java_home -v 1.8)",
    "/* pod:install */": "Sets up IOS Pods",
    "pod:install": "cd ios/ && pod install && cd ..",
    "start": "rm -rf ./node_modules/.cache/babel-loader && rm -rf /tmp/metro-* && rm -rf /tmp/react-native-packager-cache-* && npm cache clean --force && react-native start --resetCache",
    "start:clean": "tsc && npm start -- --reset-cache",
    "ios:min": "npx react-native run-ios --simulator=\"iPhone 5s\"",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "clear-android": "rm -rf android/build",
    "clear-ios": "rm -rf ios/build",
    "clear-pods": "rm -rf ios/Pods ios/build ios/Podfile.lock",
    "test": "jest --config unittest-jest.config.js --coverage && tsc",
    "detox:build": "detox build --configuration ios.sim.release",
    "/* detox:test */": "test_all_files Use npm run detox:test to run all the test files,test_single_file npm run detox:test <file_name> to run a specific test file --- example : npm run detox:test login.spec.ts",
    "detox:test": "detox test --configuration ios.sim.release",
    "detox:test:verbose": "detox test --configuration ios.sim.release --loglevel verbose",
    "tsc": "tsc",
    "storybook": "start-storybook -p 6000",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start:original": "react-native start",
    "test:original": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:all": "eslint --fix && npx prettier ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:all",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@cheapreats/ts-sdk": "^1.12.11",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^5.9.6",
    "@react-native-community/push-notification-ios": "^1.4.0",
    "@react-native-community/viewpager": "^4.1.4",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/core": "^5.12.3",
    "@react-navigation/material-top-tabs": "^5.2.16",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@sentry/react-native": "^1.7.1",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-ondevice-knobs": "^5.3.19",
    "@types/detox": "^16.4.1",
    "@types/jasmine": "^3.5.12",
    "@types/lodash": "^4.14.158",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-native-awesome-card-io": "^0.8.1",
    "@types/react-native-push-notification": "^3.0.9",
    "@types/react-native-share": "^3.3.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "core-js": "^3.6.5",
    "detox": "^17.3.6",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "lodash": "^4.17.19",
    "moment": "^2.27.0",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.63.2",
    "react-native-animatable": "^1.3.3",
    "react-native-awesome-card-io": "^0.8.3",
    "react-native-camera": "^3.36.0",
    "react-native-contacts": "^5.2.5",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-device-info": "^5.6.4",
    "react-native-draggable-flatlist": "^2.3.6",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.7.0",
    "react-native-image-picker": "^2.3.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.2",
    "react-native-localize": "^1.4.1",
    "react-native-map-link": "^2.7.17",
    "react-native-maps": "0.27.1",
    "react-native-maps-directions": "^1.8.0",
    "react-native-mixpanel": "^1.2.0",
    "react-native-push-notification": "^5.0.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.1.4",
    "react-native-screens": "^2.10.1",
    "react-native-share": "^3.7.0",
    "react-native-tab-view": "^2.15.1",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^10.4.1",
    "stripe-client": "^1.1.5",
    "styled-components": "^5.1.1",
    "tipsi-stripe": "^7.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react-native": "^5.3.19",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.1",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-watch": "^7.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.62.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "sinon": "^9.0.3",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/cheapreats.app",
        "build": "xcodebuild -workspace ios/cheapreats.xcworkspace -scheme cheapreats -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "id": "D53474CF-7DD1-4673-8517-E75DAD6C34D6",
          "type": "iPhone 11 Pro",
          "name": "MySim",
          "os": "iOS 13.0"
        }
      },
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/cheapreats.app",
        "build": "xcodebuild -workspace ios/cheapreats.xcworkspace -scheme cheapreats -configuration Release -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 11 Pro Max"
        }
      }
    },
    "runner-config": "./__e2eTests__/config.json",
    "test-runner": "jest"
  }
}
